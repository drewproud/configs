[alias]
  st = status
  ci = commit -v
  br = branch
  poh = push origin head --no-verify
  co = checkout
[core]
  excludesfile = /Users/proud/.gitignore_global
  editor = vim
[user]
	name = Drew Proud
	email = proud@uber.com
[color]
  branch = auto
  diff = auto
  status = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[branch]
	autosetuprebase = always
[rerere]
	enabled = true
[http "https://gopkg.uberinternal.com"]
	followRedirects = true
[http "https://gopkg.in"]
	followRedirects = true
